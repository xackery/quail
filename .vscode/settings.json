{
    "files.associations": {
        "*.log": "ini",
    },
    "gopls": {
        "buildFlags": ["-tags", "test_all"],
    },
    "go.testEnvVars": {
        "SINGLE_TEST": "1"
    },
    "go.testEnvFile": "${workspaceFolder}/.env",
    "workbench.editorAssociations": {
        "*.hex": "hexEditor.hexedit",
        "*.eqg": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.s3d": "hexEditor.hexedit",
       // "*.mod": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.mds": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.ter": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        //"*.zon": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.lay": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.pts": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.prt": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.dat": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.pprof": "easyview.view",
        //"*.wld": "hexEditor.hexedit", //"binary-file-viewer.viewer",
    },
    "files.trimTrailingWhitespace": true,
    "binary-file-viewer.parserFolders": [
        ".vscode/parsers"
    ],
    "search.exclude": {
        "**/.git": true,
        "**/test_data": true,
    },
    "window.title": "quail",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#00283c",
        "activityBar.background": "#00283c",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#bf007f",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#00283c",
        "statusBar.background": "#000609",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#00283c",
        "statusBarItem.remoteBackground": "#000609",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#000609",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#00060999",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#000609"
}